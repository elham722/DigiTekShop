{
  "ConnectionStrings": {
  
  },
  "JwtSettings": {
    "Issuer": "DigiTekShop",
    "Audience": "DigiTekShopClient",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 30
  },
  "PasswordPolicy": {
    "MinLength": 12,
    "RequiredCategoryCount": 3,
    "MaxRepeatedChar": 3,
    "HistoryDepth": 5,
    "ForbidUserNameFragments": true,
    "ForbidEmailLocalPart": true,
    "Blacklist": [
      "password", "123456", "qwerty", "admin", "welcome",
      "iloveyou", "letmein", "abc123", "111111", "123123",
      "qwertyui", "654321", "hello123", "monkey123", "dragon123",
      "master", "696969", "mustang", "letmein", "shadow",
      "ashley", "pokemon", "superman", "killer", "trustno1"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
